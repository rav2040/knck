{
  "name": "knck",
  "version": "1.2.2",
  "description": "knck - a URL-shortening service",
  "private": true,
  "author": "Stuart Kennedy",
  "license": "MIT",
  "homepage": "https://github.com/rav2040/knck",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rav2040/knck.git"
  },
  "bugs": {
    "url": "https://github.com/rav2040/knck/issues"
  },
  "main": "app/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "npm run start:ci & wait-on http-get://localhost:3000 && cypress run",
    "cy:open": "cypress open",
    "build": "rm -rf ./app && tsc -b",
    "start": "npm run build && pm2 restart ecosystem.config.yml",
    "start:dev": "nodemon -w ./src/index.ts -e ts-node -r dotenv/config ./src/index.ts",
    "start:ci": "npm run build && node -r dotenv/config ./app/index.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "clipboardy": "^2.3.0",
    "cypress": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "wait-on": "^5.2.0"
  },
  "peerDependencies": {
    "pm2": "^4.4.0"
  },
  "dependencies": {
    "@tuft/helmet-prehandler": "^1.0.0",
    "@tuft/view-responder": "^1.1.1",
    "aws-sdk": "^2.748.0",
    "tuft": "^2.0.0-beta.7"
  }
}
